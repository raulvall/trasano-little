<?xml version='1.0' encoding='utf-8'?>
<widget id="es.uem.tfg.trasano" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>trasano</name>
    <description>Transporte Sanitario no Urgente</description>
    <author email="trasano.uem@gmail.com" href="http://trasano.esy.es">Trasano Team</author>
    <content src="index.html" />
    <gap:plugin name="phonegap-nfc" source="npm" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="Orientation" value="portrait" />
</widget>
